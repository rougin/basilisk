{
    "name": "rougin/basilisk",
    "description": "A skeleton application for the Slytherin framework.",
    "keywords": ["application", "basilisk", "skeleton", "slytherin"],
    "homepage": "https://github.com/rougin/basilisk",
    "license": "MIT",
    "authors": [
        {
            "name": "Rougin Royce Gutib",
            "email": "rougingutib@gmail.com",
            "homepage": "https://rougin.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "filp/whoops": "~1.0|~2.0",
        "illuminate/database": "~5.0",
        "illuminate/events": "~5.0",
        "illuminate/view": "~5.0",
        "robmorgan/phinx": "~0.9",
        "rougin/authsum": "dev-master",
        "rougin/weasley": "~0.6",
        "vlucas/phpdotenv": "~2.0",
        "vlucas/valitron": "~1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2|~5.7",
        "scrutinizer/ocular": "~1.1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        },
        "files": [
            "src/HelperCollection.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "tests"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}