{
    "name": "rougin/slytherin-skeleton",
    "description": "A simple Slytherin-based web application",
    "keywords": [
        "php",
        "application",
        "skeleton",
        "slytherin"
    ],
    "homepage": "https://github.com/rougin/slytherin-skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Rougin Royce Gutib",
            "email": "rougingutib@gmail.com",
            "homepage": "http://rougin.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "container-interop/container-interop": "^1.1",
        "doctrine/orm": "^2.5",
        "filp/whoops": "^1.1",
        "hassankhan/config": "^0.8.2",
        "jms/serializer": "^1.0",
        "rougin/slytherin": "^0.5.0",
        "vlucas/phpdotenv": "^2.2",
        "vlucas/valitron": "^1.2",
        "zendframework/zend-diactoros": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
