{
    "name": "rougin/slytherin-skeleton",
    "description": "A simple web application skeleton on top of Slytherin PHP framework.",
    "keywords": [
        "php framework",
        "skeleton application",
        "slytherin",
        "web application"
    ],
    "homepage": "https://github.com/rougin/slytherin-skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Rougin Royce Gutib",
            "email": "rougingutib@gmail.com",
            "homepage": "http://rougin.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "http-interop/http-middleware": "~0.4",
        "rougin/slytherin": "dev-master",
        "vlucas/phpdotenv": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpunit/phpunit-selenium": "~2.0",
        "scrutinizer/ocular": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Skeleton\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Skeleton\\": "tests"
        }
    }
}
