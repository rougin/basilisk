{
    "name": "rougin/slytherin-skeleton",
    "description": "A simple web application skeleton on top of Slytherin framework",
    "keywords": [
        "php",
        "application",
        "skeleton",
        "slytherin"
    ],
    "homepage": "https://github.com/rougin/slytherin-skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Rougin Royce Gutib",
            "email": "rougingutib@gmail.com",
            "homepage": "http://rougin.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "container-interop/container-interop": "~1.1",
        "illuminate/database": "5.0.*",
        "filp/whoops": "~1.0",
        "robmorgan/phinx": "~0.6",
        "rougin/slytherin": "dev-master",
        "twig/twig": "~1.2",
        "vlucas/phpdotenv": "~2.2",
        "vlucas/valitron": "~1.2",
        "zendframework/zend-diactoros": "~1.3",
        "zendframework/zend-stratigility": "~1.2"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0",
        "phpunit/phpunit-selenium": "~2.0",
        "scrutinizer/ocular": "~1.0"
    },
    "autoload": {
        "files": [
            "src/Helpers/BasePathHelper.php",
            "src/Helpers/ConfigHelper.php",
            "src/Helpers/MiddlewareHelper.php",
            "src/Helpers/RedirectHelper.php",
            "src/Helpers/RequestHelper.php",
            "src/Helpers/SessionHelper.php",
            "src/Helpers/ValidateHelper.php",
            "src/Helpers/ViewHelper.php"
        ],
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
